{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2",
  ],
  "plugins": ["transform-runtime",
      ["component", [
        {
          "libraryName": "mint-ui",
          "style": true
        }
      ]]
  ],
  "comments": false,
  "env": {
    "modules": false,
    "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
    },
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
